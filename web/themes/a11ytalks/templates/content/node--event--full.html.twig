{% embed "a11ytalks:event" with {
  body: content.body,
  field_date_time: content.field_event_date,
  field_link: content.field_link|field_value,
  field_media: content.field_media,
  field_speakers: content.field_speakers,
  field_youtube: content.field_youtube,
  date_time: node.field_date_time.0.value
} %}
  {% block field_media %}
    {{ content.field_media }}
  {% endblock %}
  {% block field_tags %}
    {{ content.field_tags }}
  {% endblock %}
{% endembed %}


{
    "name": "a11ytalks/a11ytalks-drupal",
    "description": "The Drupal version of a11ytalks.com",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://a11ytalks.com",
    "authors": [
        {
            "name": "Mark Casias",
            "email": "markie@teampoop.com",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/a11ytalks/a11ytalks-drupal/issues"
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/a11ytalks"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.4",
        "drupal/ckeditor_abbreviation": "^3.0",
        "drupal/core-composer-scaffold": "^10.2",
        "drupal/core-recommended": "^10.2",
        "drupal/core-vendor-hardening": "^10.2",
        "drupal/editoria11y": "^2.1",
        "drupal/feeds": "^3.0@beta",
        "drupal/gin": "^3.0@rc",
        "drupal/gin_toolbar": "^1.0@rc",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/link_attributes": "^2.1",
        "drupal/menus_attribute": "^1.3",
        "drupal/metatag": "^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_process_markdown_to_html": "^1.1",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/nomarkup": "^1.0",
        "drupal/pathauto": "^1.12",
        "drupal/sdc_examples": "@dev",
        "drupal/smart_trim": "^2.1",
        "drupal/storybook": "^1.0@alpha",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drush/drush": "^12.4"
    },
    "require-dev": {
        "drupal/core-dev": "^10.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-vendor-hardening": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "enable-patching": true,
        "patches": {},
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
